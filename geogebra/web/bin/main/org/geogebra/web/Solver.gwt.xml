<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='solver'>
    <inherits name='org.geogebra.keyboard.KeyboardWeb'/>
    <inherits name='org.geogebra.web.Html5' />
    <inherits name="com.himamis.retex.editor.JLMEditorDev" />

    <!-- Specify the paths for translatable code                    -->
    <source path='editor'/>
    <source path='plugin'/>
    <source path='solver'/>
    <source path='shared'/>
    <public path="pub" />
    <resource path='resources'/>
    <resource path='pub/js'/>
    <super-source path='html5-super' />
    <entry-point class='org.geogebra.web.solver.StepsEntry'/>
    <!-- #3511 -->
    <set-property name="compiler.stackMode" value="strip"/>

    <replace-with class="org.geogebra.web.html5.factories.NoCASFactory">
        <when-type-is class="org.geogebra.common.factories.CASFactory" />
    </replace-with>

</module>
