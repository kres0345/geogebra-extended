<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='tabletWin'>
  <inherits name='org.geogebra.web.Tablet3D'/>
  
  <replace-with class="org.geogebra.web.tablet.main.TabletDeviceWin">
    <when-type-is class="org.geogebra.web.tablet.main.TabletDevice" />    
  </replace-with>
  
  <replace-with class="org.geogebra.web.tablet.TabletWinLookAndFeel">
    <when-type-is class="org.geogebra.web.tablet.TabletLookAndFeel" />    
  </replace-with>
  
  <!-- Compile only one permutation -->
  <collapse-all-properties />
</module>
